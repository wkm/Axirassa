<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zanoccio.axirassa.domain">
	<class name="UserAccessEvent" table="UserAccessEvents">
		<id name="ID" column="USERACCESSEVENT_ID">
			<generator class="native"/>
		</id>
		
		<property name="ipaddress"/>
		<property name="date" type="timestamp" column="event_timestamp"/>
		
	</class>
</hibernate-mapping>