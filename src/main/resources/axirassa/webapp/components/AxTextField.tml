<?xml version="1.0" encoding="utf-8" ?> 
<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<t:AxFormField>
		<t:label t:for="textfield" />
		<div class="ax_input">
			<t:error
				class='literal:errorMsg'
				for="textfield"
			/>
			<t:textfield
				t:id="textfield"
				class='input txtinput' 
				t:clientId="${clientId}"
			/>
		</div>
	</t:AxFormField>
</t:container>