<?xml version="1.0" encoding="utf-8"?>
<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<t:AxFormField>
		<t:label t:for="txtfield">
			${name}
		</t:label>
		<div class='ax_input'>
			<t:error
				class='literal:errorMsg'
				id="${txtfield}-msg"
				for="txtfield"
			/>
			<t:password 
				class='input txtinput' 
				t:label="${label}" 
				t:value="text" 
				t:id="txtfield"
			/>
		</div>
	</t:AxFormField>
</t:container>