<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
	<head>
		<title>axirassa | internal systems monitor</title>
		<link rel="stylesheet" type="text/css" href="css/sentinel.css" />
	</head>
	<body>
		<div id="header">
			<span class="logo">axirassa</span> sentinel &#8212; <span class="host">zanoccio.com</span>
		</div>
		
		<t:axplotgroup>
			<div class="agent">
				<div class="agentname">CPU</div>
				<t:axplot t:id="cpuupdate" detailsHeader="Breakdown by Processor"/>
			</div>
			 
			<div class="agent">
				<div class="agentname">Memory</div>
				<t:axplot t:id="memupdate"/>
			</div>
			<div class="agent">
				<div class="agentname">Disk Space</div>
				<t:axplot t:id="diskspaceupdate" detailsHeader="Breakdown by Disk"/>
			</div>
			
			<div class="agent">
				<div class="agentname">Disk IO</div>
				<t:axplot t:id="diskioupdate" detailsHeader="Breakdown by Disk" />
			</div>
			<div class="agent">
				<div class="agentname">Network</div>
				<t:axplot t:id="networkupdate" detailsHeader="Breakdown by Network Interface" />
			</div>
			<div class="agent">
				<div class="agentname">Throughput</div>
				<t:axplot t:id="networkioupdate" detailsHeader="Breakdown by Network Interface" />
			</div>
		</t:axplotgroup>
		
		<div id="footer">
			Zanoccio, LLC.
		</div>
	</body>
</html>