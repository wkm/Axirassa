<?xml version="1.0" encoding="UTF-8"?>
<html
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter"
>

	<body>
		<t:AxForm>
			<t:Textfield
				t:mixins="AxField"
				t:label="Textfield"
				t:id="textfield"
				t:validate="required"
				t:value="text" 
			/>

			<t:Textfield
				t:mixins="AxField"
				t:label="Disabled"
				t:id="textfieldDisabled"
				disabled="true"
				t:value="text2" 
			/>
			
			<t:AxSubmit label="submit" />
		</t:AxForm>
	</body>
</html>