<?xml version="1.0" encoding="UTF-8"?>
<html
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter"
>
	<body>
		<t:AxForm>
			<t:AxPasswordField
				t:label="literal:Password"
				t:id="password"
				t:validate="required"
				t:value="literal:"
			/>
			
			<t:AxPasswordField
				t:label="literal:Custom"
				t:id="custom"
				t:validate="required"
				t:value="literal:"
			/>
			
			<t:AxSubmit label="submit" />
		</t:AxForm>
	</body>
</html>