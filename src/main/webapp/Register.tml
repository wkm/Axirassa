<html t:type="layout" t:pageTitle="Register" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
	<t:axform t:id="registerForm">
		<t:axtextfield t:id="email" t:label="E-mail" />
		<t:axtextfield t:id="confirmemail" t:label="Confirm" />
		
		<t:passwordfield t:id="password" t:label="Password" />
		<t:passwordfield t:id="confirmpassword" t:label="Confirm" />
		
		<input type="submit" value="Submit"/>
	</t:axform>
</html>