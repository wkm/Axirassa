<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://directwebremoting.org/schema/dwr30.dtd">
<dwr>
	<init>
		<creator id="singleton" class="org.directwebremoting.create.SingletonCreator"/>
	</init>
	<allow>
	    <!-- <filter class="org.directwebremoting.filter.AuditLogAjaxFilter"/> -->
	
		<create creator="singleton" scope="application" javascript="PingerLevelDataStream">
			<param name="class" value="axirassa.webapp.ajax.PingerLevelDataStream"/>
		</create>
		
		<filter class="axirassa.webapp.ajax.util.StreamCloseFilter" />
		
		<!-- DISABLE ON PRODUCTION -->
		<convert converter="exception" match="java.lang.Exception" />
		<convert converter="bean" match="java.lang.StackTraceElement" />
	</allow>
</dwr>